{
  "editor.maxTokenizationLineLength": 100000,
  "editor.formatOnSave": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontLigatures": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.hideCursorInOverviewRuler": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.gpuAcceleration": "on",
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "'Hack Nerd Font Mono', monospace",
  "scm.inputFontFamily": "",
  "debug.console.fontFamily": "",
  "markdown.preview.fontFamily": "sans-serif",
  "diffEditor.codeLens": true,
  "editor.inlineSuggest.enabled": true,
  "files.maxMemoryForLargeFilesMB": 6144,
  "workbench.editor.closeOnFileDelete": true,
  "terminal.integrated.scrollback": 100000,
  "explorer.confirmDelete": false,
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "javascript.format.semicolons": "insert",
  "typescript.format.semicolons": "remove",
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "~/css/"
    }
  ],
  "editor.minimap.size": "fit",
  "diffEditor.wordWrap": "off",
  "liveSassCompile.settings.autoprefix": [],
  "terminal.integrated.tabs.location": "left",
  "security.workspace.trust.untrustedFiles": "open",
  "prettier.singleQuote": false,
  "prettier.jsxSingleQuote": false,
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "always",
  "prettier.quoteProps": "consistent",
  "prettier.trailingComma": "all",
  "redhat.telemetry.enabled": true,
  "files.associations": {
    "*.html": "html",
    ".env": "shellscript",
    ".dev.env": "shellscript",
    ".env.dev.local": "dotenv",
    ".env.dev": "dotenv",
    "*.js": "javascript"
  },
  "prettier.printWidth": 80,
  "yaml.format.printWidth": 80,
  "editor.wordWrapColumn": 80,
  "markdown.extension.italic.indicator": "_",
  "typescript.locale": "en",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.jsxAttributeCompletionStyle": "braces",
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.format.insertSpaceAfterConstructor": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "typescript.tsserver.maxTsServerMemory": 2048,
  "vsintellicode.features.python.deepLearning": "enabled",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4,
    "editor.formatOnType": true
  },
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "[c++]": {
    "editor.defaultFormatter": null,
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.defaultFormatter": null,
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4
  },
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.tabSize": 4,
    "editor.defaultFormatter": "golang.go"
  },
  "[yaml]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "json.format.enable": true,
  "git.autofetch": true,
  "gitlens.codeLens.enabled": false,
  "scm.inputFontSize": 14,
  "editor.inlayHints.enabled": "on",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.codeLens": true,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "json.maxItemsComputed": 100000,
  "editor.linkedEditing": true,
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "terminal.integrated.detectLocale": "on",
  "editor.minimap.renderCharacters": false,
  "editor.wrappingStrategy": "advanced",
  "terminal.integrated.lineHeight": 0,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
